pipeline {
    agent any
    stages {
        stage ('checkout') {
            steps {
            git branch: 'main', url: 'https://github.com/tengaleh/assignment11.git'
            }
        }
        stage ('packaging') {
             steps {
    
            sh 'mvn clean package'
        }
    }
        
        stage ('archving') {
            steps {
                 archiveArtifacts 'target/*.jar'
            }
        }
    }
}
